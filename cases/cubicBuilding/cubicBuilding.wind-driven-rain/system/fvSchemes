FoamFile
{
  version 2.0;
  format ascii;
  class dictionary;
  location "system";
  object fvSchemes;
}

ddtSchemes
{
  default steadyState;
}

gradSchemes
{
  default Gauss linear;
}

divSchemes
{
  default none;

  div(phi,alphar) bounded Gauss vanLeer;          
  div(phi,Ur) bounded Gauss vanLeerV;

  div((-nutrain*T(grad(Ur)))) Gauss linear;
}

laplacianSchemes
{
  default Gauss linear orthogonal;
}

interpolationSchemes
{
  default linear;
}

snGradSchemes
{
  default orthogonal;
}

