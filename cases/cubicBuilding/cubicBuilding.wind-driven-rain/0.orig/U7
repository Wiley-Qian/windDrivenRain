FoamFile
{
  version 2.0;
  format ascii;
  class volVectorField;
  object U7;
}

dimensions [0 1 -1 0 0 0 0];
internalField uniform (1 0 0);

boundaryField
{
  inlet
  {
    type mapped;
    interpolationScheme cell;
    setAverage false;
    average ( 1 0 0 );
    value uniform (1 -3.52235726598264 0);
  }

  top
  {
    type mapped;
    interpolationScheme cell;
    setAverage false;
    average ( 1 0 0 );
    value uniform (1 -3.52235726598264 0);
  }

  #include "include/U"
}

