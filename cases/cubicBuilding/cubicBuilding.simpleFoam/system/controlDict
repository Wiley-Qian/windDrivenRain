FoamFile
{
  version 2.0;
  format ascii;
  class dictionary;
  location "system";
  object controlDict;
}

application     simpleFoam;

startFrom       latestTime;
startTime       0;
stopAt          endTime;
endTime         10000;
deltaT          1;

writeControl    timeStep;
writeInterval   1000;
purgeWrite      0;
writeFormat     binary;
writePrecision  6;
writeCompression uncompressed;

timeFormat      general;
timePrecision   6;

runTimeModifiable true;

